         <foreach collection="ids" item = "id" open="(" separator="," close=")">
         #{id}
         </foreach>
    </update>
  	<delete id="delete" flushCache="true">
      	delete  from cms_channel_big where id =#{id}
    </delete>
  	<delete id="deleteBatch" flushCache="true">
      	delete  from cms_channel_big where id in
        <foreach collection="array" item = "ids" open="(" separator="," close=")">
        #{ids}
        </foreach>
    </delete>
    <select id="find" resultMap="channelBigResult" parameterType="java.util.Map">
        SELECT <include refid="allColumns"/> FROM cms_channel_big
        <where>
            and available=1
            and deleted=0
            <include refid="ChannelBig_Where_Clause"/>
        </where>
    </select>
