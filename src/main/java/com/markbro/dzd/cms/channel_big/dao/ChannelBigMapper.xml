	<update id="updateSort" parameterType="java.lang.String">
		update cms_channel_big set sort=#{sort} where id=#{id}
	</update>
	<select id="getMaxSort" resultType="java.lang.Integer">
		select IFNULL(Max(sort),0) from cms_channel_big where deleted=0
	</select>
