<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC   
    "-//mybatis.org//DTD Mapper 3.0//EN"  
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.markbro.dzd.cms.channel_big.dao.ChannelBigMapper">
	<!--<sql id="addColumns">id,name,jump_url,img_url,small_img_url,sort,subscribe_num,createTime,createBy,updateBy,updateTime,available,deleted</sql> -->
	<!--<sql id="updateColumns">name,jump_url,img_url,small_img_url,sort,subscribe_num,createBy,updateBy,updateTime,available,deleted</sql> -->
	<cache type="com.markbro.asoiaf.mybatis.cache.AsoiafCache" eviction="org.apache.ibatis.cache.decorators.LruCache" readOnly="true" size="1024" flushInterval="3600000"/>
	<sql id="allColumns">id,name,jump_url,img_url,small_img_url,sort,subscribe_num,createTime,createBy,updateBy,updateTime,available,deleted</sql>
	<!--如果实体类中属性名与数据库表中的字段名不一致就需要用resultMap映射-->
	<resultMap id="channelBigResult" type="com.markbro.dzd.cms.channel_big.bean.ChannelBig">
		<result property="id" column="id" />
		<result property="name" column="name" />
		<result property="jump_url" column="jump_url" />
		<result property="img_url" column="img_url" />
		<result property="small_img_url" column="small_img_url" />
		<result property="sort" column="sort" />
		<result property="subscribe_num" column="subscribe_num" />
		<result property="createTime" column="createTime" />
		<result property="createBy" column="createBy" />
		<result property="updateBy" column="updateBy" />
		<result property="updateTime" column="updateTime" />
		<result property="available" column="available" />
		<result property="deleted" column="deleted" />
	</resultMap>
    <sql id="ChannelBig_Where_Clause">
        <if test="id != null and id != ''">
				AND id = #{id}
		</if>
			<if test="name != null and name != ''">
				AND name like concat(concat('%',#{name}),'%')
		</if>
			<if test="jump_url != null and jump_url != ''">
				AND jump_url = #{jump_url}
		</if>
			<if test="img_url != null and img_url != ''">
				AND img_url = #{img_url}
		</if>
			<if test="small_img_url != null and small_img_url != ''">
				AND small_img_url = #{small_img_url}
		</if>
			<if test="sort != null and sort != ''">
				AND sort = #{sort}
		</if>
			<if test="subscribe_num != null and subscribe_num != ''">
				AND subscribe_num = #{subscribe_num}
		</if>
			<if test="createTime != null and createTime != ''">
				AND createTime = #{createTime}
		</if>
			<if test="createBy != null and createBy != ''">
				AND createBy = #{createBy}
		</if>
			<if test="updateBy != null and updateBy != ''">
				AND updateBy = #{updateBy}
		</if>
			<if test="updateTime != null and updateTime != ''">
				AND updateTime = #{updateTime}
		</if>
			<if test="available != null and available != ''">
				AND available = #{available}
		</if>
			<if test="deleted != null and deleted != ''">
				AND deleted = #{deleted}
		</if>
			
    </sql>
    <select id="get"  resultMap="channelBigResult">
        select <include refid="allColumns"/>
        from cms_channel_big
        where
        id =#{id}
    </select>
    <select id="getMap"  resultType="java.util.Map">
            select <include refid="allColumns"/>
            from cms_channel_big
            where
            id =#{id}
    </select>
    <insert id="add" useGeneratedKeys="true" keyProperty="id" parameterType="com.markbro.dzd.cms.channel_big.bean.ChannelBig" flushCache="true">
    	insert into cms_channel_big(id,name,jump_url,img_url,small_img_url,sort,subscribe_num,createTime,createBy,updateBy,updateTime,available,deleted)
    	values (#{id},#{name},#{jump_url},#{img_url},#{small_img_url},#{sort},#{subscribe_num},now(),#{createBy},#{updateBy},now(),1,0)
    </insert>
    <insert id="addByMap" useGeneratedKeys="true" keyProperty="id" parameterType="java.util.Map" flushCache="true">
    	insert into cms_channel_big(id,name,jump_url,img_url,small_img_url,sort,subscribe_num,createTime,createBy,updateBy,updateTime,available,deleted)
    	values (#{id},#{name},#{jump_url},#{img_url},#{small_img_url},#{sort},#{subscribe_num},now(),#{createBy},#{updateBy},now(),1,0)
    </insert>
    <insert id="addBatch" flushCache="true">
        insert into cms_channel_big (id,name,jump_url,img_url,small_img_url,sort,subscribe_num,createTime,createBy,updateBy,updateTime,available,deleted)
        values
        <foreach collection="list" item="item" separator="," >
        	(#{item.id},#{item.name},#{item.jump_url},#{item.img_url},#{item.small_img_url},#{item.sort},#{item.subscribe_num},now(),#{item.createBy},#{item.updateBy},now(),1,0)
        </foreach>
    </insert>
    <update id="update" parameterType="com.markbro.dzd.cms.channel_big.bean.ChannelBig" flushCache="true">
    	update cms_channel_big 
    	<set>
    		<if test="name != null">name=#{name},</if>
			<if test="jump_url != null">jump_url=#{jump_url},</if>
			<if test="img_url != null">img_url=#{img_url},</if>
			<if test="small_img_url != null">small_img_url=#{small_img_url},</if>
			<if test="sort != null">sort=#{sort},</if>
			<if test="subscribe_num != null">subscribe_num=#{subscribe_num},</if>
			<if test="createBy != null">createBy=#{createBy},</if>
			<if test="updateBy != null">updateBy=#{updateBy},</if>
			updateTime=now(),
			<if test="available != null">available=#{available},</if>
			<if test="deleted != null">deleted=#{deleted},</if>
			
    	</set>
    	 where
    	id =#{id}
    </update>
	<update id="updateByMap" parameterType="map" flushCache="true">
        update cms_channel_big
        <set>
            <if test="name != null">name=#{name},</if>
