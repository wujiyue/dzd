  	<delete id="delete" flushCache="true">
      	delete  from cms_image where id =#{id}
    </delete>
  	<delete id="deleteBatch" flushCache="true">
      	delete  from cms_image where id in
        <foreach collection="array" item = "ids" open="(" separator="," close=")">
        #{ids}
        </foreach>
    </delete>
    <select id="find" resultMap="imageResult" parameterType="java.util.Map">
        SELECT <include refid="allColumns"/> FROM cms_image
        <where>
            and available=1
            and deleted=0
            <include refid="Image_Where_Clause"/>
        </where>
    </select>
