<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC   
    "-//mybatis.org//DTD Mapper 3.0//EN"  
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.markbro.dzd.base.dictionary.dao.DictionaryMapper">
	<!--<sql id="addColumns">id,type,value,label,description,parentId,createTime,updateTime,createBy,updateBy,sort,available,deleted</sql> -->
	<!--<sql id="updateColumns">type,value,label,description,parentId,updateTime,createBy,updateBy,sort,available,deleted</sql> -->
	<cache type="com.markbro.asoiaf.mybatis.cache.AsoiafCache" eviction="org.apache.ibatis.cache.decorators.LruCache" readOnly="true" size="1024" flushInterval="3600000"/>
	<sql id="allColumns">id,type,value,label,description,parentId,createTime,updateTime,createBy,updateBy,sort,available,deleted</sql>
	<!--如果实体类中属性名与数据库表中的字段名不一致就需要用resultMap映射-->
	<resultMap id="dictionaryResult" type="com.markbro.dzd.base.dictionary.bean.Dictionary">
		<result property="id" column="id" />
		<result property="type" column="type" />
		<result property="value" column="value" />
		<result property="label" column="label" />
		<result property="description" column="description" />
		<result property="parentId" column="parentId" />
		<result property="createTime" column="createTime" />
		<result property="updateTime" column="updateTime" />
		<result property="createBy" column="createBy" />
		<result property="updateBy" column="updateBy" />
		<result property="sort" column="sort" />
		<result property="available" column="available" />
		<result property="deleted" column="deleted" />
	</resultMap>
    <sql id="Dictionary_Where_Clause">
        <if test="id != null and id != ''">
				AND id = #{id}
		</if>
			<if test="type != null and type != ''">
				AND type = #{type}
		</if>
			<if test="value != null and value != ''">
				AND value = #{value}
		</if>
			<if test="label != null and label != ''">
				AND label = #{label}
		</if>
			<if test="description != null and description != ''">
				AND description = #{description}
		</if>
			<if test="parentId != null and parentId != ''">
				AND parentId = #{parentId}
		</if>
			<if test="createTime != null and createTime != ''">
				AND createTime = #{createTime}
		</if>
			<if test="updateTime != null and updateTime != ''">
				AND updateTime = #{updateTime}
		</if>
			<if test="createBy != null and createBy != ''">
				AND createBy = #{createBy}
		</if>
			<if test="updateBy != null and updateBy != ''">
				AND updateBy = #{updateBy}
		</if>
			<if test="sort != null and sort != ''">
				AND sort = #{sort}
		</if>
			<if test="available != null and available != ''">
				AND available = #{available}
		</if>
			<if test="deleted != null and deleted != ''">
				AND deleted = #{deleted}
		</if>
			
    </sql>
    <select id="get"  resultMap="dictionaryResult">
        select <include refid="allColumns"/>
        from base_dictionary
        where
        id =#{id}
    </select> 
    <insert id="add" useGeneratedKeys="true" keyProperty="id" parameterType="com.markbro.dzd.base.dictionary.bean.Dictionary" flushCache="true">
    	insert into base_dictionary(id,type,value,label,description,parentId,createTime,updateTime,createBy,updateBy,sort,available,deleted)
    	values (null,#{type},#{value},#{label},#{description},#{parentId},now(),now(),#{createBy},#{updateBy},#{sort},1,0)
    </insert>
    <insert id="addBatch" flushCache="true">
        insert into base_dictionary (id,type,value,label,description,parentId,createTime,updateTime,createBy,updateBy,sort,available,deleted)
        values
        <foreach collection="list" item="item" separator="," >
        	(null,#{item.type},#{item.value},#{item.label},#{item.description},#{item.parentId},now(),now(),#{item.createBy},#{item.updateBy},#{item.sort},1,0)
        </foreach>
    </insert>
    <update id="update" parameterType="com.markbro.dzd.base.dictionary.bean.Dictionary" flushCache="true">
    	update base_dictionary 
    	<set>
    		<if test="type != null">type=#{type},</if>
			<if test="value != null">value=#{value},</if>
			<if test="label != null">label=#{label},</if>
			<if test="description != null">description=#{description},</if>
			<if test="parentId != null">parentId=#{parentId},</if>
			updateTime=now(),
			<if test="createBy != null">createBy=#{createBy},</if>
			<if test="updateBy != null">updateBy=#{updateBy},</if>
			<if test="sort != null">sort=#{sort},</if>
			<if test="available != null">available=#{available},</if>
			<if test="deleted != null">deleted=#{deleted},</if>
			
    	</set>
    	 where
    	id =#{id}
    </update>
	<update id="updateByMap" parameterType="map" flushCache="true">
        update base_dictionary
        <set>
            <if test="type != null">type=#{type},</if>
			<if test="value != null">value=#{value},</if>
			<if test="label != null">label=#{label},</if>
			<if test="description != null">description=#{description},</if>
			<if test="parentId != null">parentId=#{parentId},</if>
			updateTime=now(),
			<if test="createBy != null">createBy=#{createBy},</if>
			<if test="updateBy != null">updateBy=#{updateBy},</if>
			<if test="sort != null">sort=#{sort},</if>
			<if test="available != null">available=#{available},</if>
			<if test="deleted != null">deleted=#{deleted},</if>
			
        </set>
        where
        id =#{id}
    </update>
	<update id="updateByMapBatch" parameterType="map" flushCache="true">
         update base_dictionary
         <set>
         <if test="type != null">type=#{type},</if>
			<if test="value != null">value=#{value},</if>
			<if test="label != null">label=#{label},</if>
			<if test="description != null">description=#{description},</if>
			<if test="parentId != null">parentId=#{parentId},</if>
			updateTime=now(),
			<if test="createBy != null">createBy=#{createBy},</if>
			<if test="updateBy != null">updateBy=#{updateBy},</if>
			<if test="sort != null">sort=#{sort},</if>
			<if test="available != null">available=#{available},</if>
			<if test="deleted != null">deleted=#{deleted},</if>
			
         </set>
         where
         id in
         <foreach collection="ids" item = "id" open="(" separator="," close=")">
         #{id}
         </foreach>
    </update>
  	<delete id="delete" flushCache="true">
      	delete  from base_dictionary where id =#{id}
    </delete>
  	<delete id="deleteBatch" flushCache="true">
      	delete  from base_dictionary where id in
        <foreach collection="array" item = "ids" open="(" separator="," close=")">
        #{ids}
        </foreach>
    </delete>
    <select id="find" resultMap="dictionaryResult" parameterType="java.util.Map">
        SELECT <include refid="allColumns"/> FROM base_dictionary
        <where>
            and available=1
            and deleted=0
            <include refid="Dictionary_Where_Clause"/>
        </where>
    </select>
    <select id="findByMap" resultMap="dictionaryResult"  parameterType="map" >
   		SELECT <include refid="allColumns"/> FROM base_dictionary
   		<where>
   		    <if test="id != null and id != ''">
				AND id = #{id}
			</if>
			<if test="type != null and type != ''">
				AND type = #{type}
			</if>
			<if test="value != null and value != ''">
				AND value = #{value}
			</if>
			<if test="label != null and label != ''">
				AND label = #{label}
			</if>
			<if test="description != null and description != ''">
				AND description = #{description}
			</if>
			<if test="parentId != null and parentId != ''">
				AND parentId = #{parentId}
			</if>
			<if test="createTime != null and createTime != ''">
				AND createTime = #{createTime}
			</if>
			<if test="updateTime != null and updateTime != ''">
				AND updateTime = #{updateTime}
			</if>
			<if test="createBy != null and createBy != ''">
				AND createBy = #{createBy}
			</if>
			<if test="updateBy != null and updateBy != ''">
				AND updateBy = #{updateBy}
			</if>
			<if test="sort != null and sort != ''">
				AND sort = #{sort}
			</if>
			<if test="available != null and available != ''">
				AND available = #{available}
			</if>
			<if test="deleted != null and deleted != ''">
				AND deleted = #{deleted}
			</if>
			
   		</where>
   	</select>

	<select id="findByType" resultType="com.markbro.dzd.base.dictionary.bean.Dictionary">
		select *
			from base_dictionary
		<where>
			type=#{type}
			and deleted=0
			and available=1
		</where>
	</select>
</mapper>
